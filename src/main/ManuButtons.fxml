<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox fx:id="ekranButtons" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.ManuButtonsController">
   <children>
      <ToggleButton fx:id="magazynButton" mnemonicParsing="false" onAction="#openMagazyn" prefHeight="80.0" prefWidth="140.0" text="Magazyn">
         <toggleGroup>
            <ToggleGroup fx:id="toggleButtons" />
         </toggleGroup></ToggleButton>
      <ToggleButton fx:id="sklepButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#openSklep" prefHeight="80.0" prefWidth="140.0" text="Sklep" toggleGroup="$toggleButtons" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
